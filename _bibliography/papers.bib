@article{adams2022equity,
  title        = {Equity-Driven Sensing System for Measuring Skin Tone--Calibrated Peripheral Blood Oxygen Saturation (OptoBeat): Development, Design, and Evaluation Study},
  author       = {Adams, Alexander T and Mandel, Ilan and Gao, Yixuan and Heckman, Bryan W and Nandakumar, Rajalakshmi and Choudhury, Tanzeem},
  journal      = {JMIR Biomedical Engineering},
  abbr         = {JMIR Biomed Eng},
  volume       = {7},
  number       = {1},
  pages        = {e34934},
  year         = {2022},
  publisher    = {JMIR Publications Inc., Toronto, Canada},
  doi          = {10.2196/34934},
  url          = {https://biomedeng.jmir.org/2022/1/e34934/},
  html         = {https://biomedeng.jmir.org/2022/1/e34934/},
  preview      = {optobeat.png},
  abstract     = {Background: Pulse oximetry is a noninvasive method for monitoring blood oxygen saturation that is essential for clinical care. However, existing devices exhibit systematic biases against patients with darker skin tones, leading to healthcare disparities. Objective: We aimed to develop an equity-driven sensing system for measuring skin tone-calibrated peripheral blood oxygen saturation. Methods: We developed OptoBeat, a novel pulse oximetry system that incorporates skin tone measurement and calibration algorithms to provide more accurate oxygen saturation readings across all skin tones. Results: Our evaluation shows improved accuracy and reduced bias across diverse skin tones compared to conventional pulse oximeters. Conclusions: OptoBeat represents a significant step toward more equitable medical sensing technology.},
  bibtex_show  = {true},
  dimensions   = {true},
  altmetric    = {true},
  selected     = {true}
}

